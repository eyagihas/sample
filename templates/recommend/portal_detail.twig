{% extends "common/portal/base.twig" %}
{% block format_detection %}
<meta name="format-detection" content="telephone=no">
{% endblock %}
{% block robots %}
<meta name="robots" content="{{value.headline.ga_index}}">
{% endblock %}
{% block description %}
<meta name="description" content="{{value.recommend.description}}" />
{% endblock %}
{% block keyword %}
<meta name="keywords" content="{{value.recommend.keyword}}" />
{% endblock %}
{% block canonical %}
{% if value.headline.canonical != '' %}
<link rel="canonical" href="{{value.headline.canonical}}" />
{% endif %}
{% endblock %}
{% block thumbnail %}
<meta name="thumbnail" content="{{value.headline.base_url}}{{value.recommend.thumbnail_url}}">
{% endblock %}
{% block json_ld %}
{% include 'recommend/json_ld.html' %}
{% endblock %}
{% block external_js %}
