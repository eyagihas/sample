			letter-spacing: 0.1em;
			color: #212529;
			border-radius: 0.5rem;
		}

		.btn-wrap-pc-sp {
			max-width: 435px;
			margin: 0 auto 20px;
		}

		a.btn-pc-sp {
			line-height: 1.4;
			width: 49.5%;
			padding: 1.25rem 0;
			color: #ffff;
		}

		a.btn--contact {
			display: block;
			padding: 1.5rem 0;
			color: #fff;
			background: #d20010;
			-webkit-box-shadow: 0 5px 0 #b9000e;
			box-shadow: 0 5px 0 #b9000e;
		}

		a.btn--contact i {
			font-size: 2rem;
		}

		a.btn--contact i.fa-position-left {
			font-size: 3rem;
			left: 2rem;
		}

		a.btn--contact :hover {
			color: #fff;
		}

		a.btn--tel {
			display: block;
			margin-top: 15px;
			padding: 1rem 0;
			color: #fff;
			background: #094;
			-webkit-box-shadow: 0 5px 0 #008039;
			box-shadow: 0 5px 0 #008039;
		}

		a.btn--tel i {
			font-size: 2rem;
		}

		a.btn--tel i.fa-position-left {
			font-size: 3rem;
			left: 2rem;
		}

		a.btn--tel .number {
			line-height: 1;
			display: inline-block;
			margin-top: 0.5rem;
			padding: 0.25rem 2rem;
			letter-spacing: 0;
			color: #094;
			border-radius: 0.5rem;
			background: #fff;
		}

		a.btn--tel:hover {
			color: #fff;
		}

		.fa-position-left {
			position: absolute;
			top: calc(50% - 0.5em);
			left: 1rem;
		}

		.fa-position-right {
			position: absolute;
			top: calc(50% - 0.5em);
			right: 1rem;
		}

		ul.cp_list {
			padding:0.5em;
			list-style-type:none;
		}

		ul.cp_list li {
			position:relative;
			padding: 0em 0.3em 0.3em 1.3em;
		}

		ul.cp_list li:after,ul.cp_list li:before {
			position:absolute;
			content:'';
			display:block;
			background: #FFC107;
			top: 0.43em;
			left: 0.5em;
			height: 11px;
			width: 4px;
			border-radius: 10px;
			transform: rotate(45deg);
		}

		ul.cp_list li:before {
			top: 0.6em;
			left: 0.2em;
			height:8px;
			transform:rotate(-45deg);
		}

		.table_style {
			width: 100%;
			border: 1px solid #2a4896;
			margin-bottom: 16px;
			background-color: #FFFFFF;
		}

		.table_style thead {
			text-align: center;
			line-height: 2;
			font-family: "新ゴ M", "Shin Go Medium";
			border-bottom: 1px solid #2a4896;
			background-color: #2a4896;
			color: #FFFFFF; 
